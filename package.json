{
  "name": "family-tree-eleventy-site",
  "version": "1.0.0",
  "private": true,
  "description": "Generate a family tree static site from folder-based markdown data.",
  "scripts": {
    "build": "node scripts/clean-output.js && eleventy",
    "start": "eleventy --serve",
    "photos:demo": "node scripts/generate-demo-photos.js",
    "photos:public": "node scripts/download-public-photos.js",
    "serve:secure": "node server/index.js",
    "start:secure": "npm run build && node server/index.js"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "d3-dag": "^1.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "google-auth-library": "^10.5.0",
    "markdown-it": "^14.1.0"
  }
}
